<%= semantic_form_for @campaign do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :leads_purchased %>
    <%= f.input :leads_credited %>
    <%= f.input :lead_price %>
    <%= f.input :payout %>
    <%= f.input :script %>
  <% end %>
  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
